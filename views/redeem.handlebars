<!doctype html>
<html>

    <head>
        <title>Redeem Points</title>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        <script src="https://code.jquery.com/jquery.js"></script>   
        <script src="js/bootstrap.js"></script>
        <script src="js/jquery-1.11.0.js"></script>
        <script src="js/prod-activity.js"></script>

        <link href="css/bootstrap.css" rel="stylesheet">
        <link href="css/bootstrap-theme.css" rel="stylesheet">
        <link href="css/add.css" rel="stylesheet">
        <link href="css/redeem.css" rel="stylesheet">
    </head>

    <body>

      <div class="mainbody" style = "background-color: #e6e2da">

        <div id="icon">
          <a href="/index">
            <img class="bicon" src="back.png" height=40px style = "float: left" />
          </a>
        </div>

        <div id="container">

          <div id="header">
            <h2> <b>Redeem Points</b> </h2>
          </div>
          <hr id="border"/>

        </div>
        <div id="profile">
          <img class="proPic" src="defaultpp.png">
        </div>

        <div class="listheader">
        <h4>Profile frames</h4>
        </div>
        <div class="items">
          <div class="frame">
            <div class="column">
              <img src="thuglife.png" id="thuglife">
              <h5>Thug LIfe</h5>
              <button name="button" id="thuglifeBtn">30pts</button>
            </div>
            <div class="column">
              <img src="frame.png" id="frame">
              <h5>frame</h5>
              <button name="button" id="frameBtn">80pts</button>
            </div>
            <div class="column">
              <img src="goldchain.png" id="goldchain">
              <h5>goldchain</h5>
              <button name="button" id="goldchainBtn">40pts</button>
            </div>
          </div>
        </div>

        <div class="listheader2">
        <h4>Random Picture for 5pts!</h4>
        </div>

        <div class="memePic">
          <img src="qmark.jpeg" id="memeimg" />
        </div>

        <div class="memebutton">
          <button name="button" id="MemeBtn">Get it</button>
        </div>


      </div>

        
    </body>
</html>